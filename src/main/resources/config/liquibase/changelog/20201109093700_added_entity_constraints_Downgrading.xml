<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Downgrading.
    -->
    <changeSet id="20201109093700-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="step_id"
                                 baseTableName="downgrading"
                                 constraintName="fk_downgrading_step_id"
                                 referencedColumnNames="id"
                                 referencedTableName="downgrading_step"/>

        <addForeignKeyConstraint baseColumnNames="engagement_id"
                                 baseTableName="downgrading"
                                 constraintName="fk_downgrading_engagement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="engagement"/>

    </changeSet>
</databaseChangeLog>
