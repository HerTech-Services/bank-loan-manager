<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Reject.
    -->
    <changeSet id="20201102104458-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="type_id"
                                 baseTableName="reject"
                                 constraintName="fk_reject_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reject_type"/>

        <addForeignKeyConstraint baseColumnNames="engagement_id"
                                 baseTableName="reject"
                                 constraintName="fk_reject_engagement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="engagement"/>

    </changeSet>
</databaseChangeLog>
